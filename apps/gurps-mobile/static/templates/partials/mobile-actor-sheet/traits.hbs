        {{#> components/cards/list}}Button Label{{/components/cards/list}} --}}
    {{/components/tabs/panel}}
    {{#> components/tabs/panel value="advantage"}}
      <div>PANEL 2</div>
    {{/components/tabs/panel}}
    {{#> components/tabs/panel value="skill"}}
      <div>PANEL 3</div>
    {{/components/tabs/panel}}
    {{#> components/tabs/panel value="spell"}}
      <div>PANEL 4</div>
    {{/components/tabs/panel}}
    {{#> components/tabs/panel value="equipment"}}
      <div>PANEL 5</div>
    {{/components/tabs/panel}}
  </div>
</div>