<div class="stack traits light">
  <div class="label">Traits</div>
  <div class="holder"></div>
  
  <div class="tabs" data-value="{{oocTab}}">
    {{> components/tabs/tab value="attribute" icon="attribute"}}
    {{> components/tabs/tab value="advantage" icon="advantage"}}
    {{> components/tabs/tab value="skill" icon="skill"}}
    {{> components/tabs/tab value="spell" icon="spell"}}
    {{> components/tabs/tab value="equipment" icon="equipment"}}
  </div> 

  
  <div class="panels">
    {{#> components/tabs/panel value="attribute"}}
        {{#> components/cards/list}}Button Label{{/components/cards/list}}
        {{!-- {{#> components/cards/list}}Button Label{{/components/cards/list}}
