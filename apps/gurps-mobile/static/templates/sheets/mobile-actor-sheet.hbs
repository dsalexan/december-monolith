<form class='{{cssClass}}' autocomplete='off'>
  {{group true '%c [xii/gurps-mobile/actor-sheet] %cRENDER  %c mobile-actor-sheet.hbs ' 'color: darkgray; padding-left: 4px; font-weight: 400; ' 'color: #d40c0c' 'background-color: rgba(255, 0, 0, 0.85); color: white; font-weight: bold;'}}
  {{log '[xii/gurps-mobile]' '   (handlebars) ' this level='error'}}
  {{log '[xii/gurps-mobile]' '        (actor) ' actor level='error'}}
  {{log '[xii/gurps-mobile]' '    (maneuvers) ' maneuvers level='error'}}
  {{log '[xii/gurps-mobile]' '     (maneuver) ' actor.flags.gurps.mobile.lastManeuver data.conditions level='error'}}
  {{log '[xii/gurps-mobile]' '(last-maneuver) ' data.conditions.maneuver level='error'}}
  {{log '[xii/gurps-mobile]' '      (feature) ' tabs level='error'}}
  {{groupend}}

  {{> mobile-actor-sheet/header}}
  {{> mobile-actor-sheet/stack}}
</form>